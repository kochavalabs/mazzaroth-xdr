{
  "name": "mazzaroth-xdr",
  "version": "0.1.0",
  "description": "Serialization code for mazzaroth.",
  "main": "js/xdr_generated.js",
  "scripts": {
    "clean": "rm -f ./xdr/xdr_generated.go && rm -rf ./js && mkdir js",
    "build-go": "cargo run --manifest-path=xdrgen/Cargo.toml ./idl/*.x | gofmt > ./xdr/xdr_generated.go",
    "build-js": "cargo run --manifest-path=xdrgen/Cargo.toml ./idl/*.x --language js > ./js/xdr_generated.js && babel js -d js",
    "build": "npm run clean && npm run build-js && npm run build-go",
    "write": "npm run build-js && node ./examples/write_file.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kochavalabs/mazzaroth-proto.git"
  },
  "keywords": [
    "xdr",
    "mazzaroth"
  ],
  "author": "Kyle Bartush",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kochavalabs/mazzaroth-proto/issues"
  },
  "homepage": "https://github.com/kochavalabs/mazzaroth-proto#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^5.16.0"
  },
  "dependencies": {
    "js-xdr": "^1.1.1"
  }
}
